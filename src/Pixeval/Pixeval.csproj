	</ItemGroup>
	<!-- #endregion Exposes PRI resources to the source generator -->
	<Target Name="PackageLoginProxy" BeforeTargets="PrepareForBuild">
		<PropertyGroup>
			<LoginProxyAssetsPath>$(MSBuildProjectDirectory)\Assets\Binary\Pixeval.LoginProxy.zip</LoginProxyAssetsPath>
			<LoginProxyPublishDir>$(TEMP)\LoginProxy-$(Configuration)-$(Platform)</LoginProxyPublishDir>
			<LoginProxyPublishConfiguration>$(Configuration)</LoginProxyPublishConfiguration>
			<LoginProxyPublishPlatform>$(Platform)</LoginProxyPublishPlatform>
			<LoginProxyPublishProtocol>FileSystem</LoginProxyPublishProtocol>
			<LoginProxyPublishTargetFramework>net6.0-windows</LoginProxyPublishTargetFramework>
			<LoginProxyPublishSelfContained>false</LoginProxyPublishSelfContained>
			<LoginProxyRuntimeIdentifier>$(RuntimeIdentifier)</LoginProxyRuntimeIdentifier>
		</PropertyGroup>
		<RemoveDir Directories="$(LoginProxyPublishDir)" ContinueOnError="true" />
		<MSBuild Projects="$(MSBuildProjectDirectory)\..\Pixeval.LoginProxy\Pixeval.LoginProxy.csproj" Targets="publish" Properties="Configuration=$(LoginProxyPublishConfiguration);RuntimeIdentifier=$(LoginProxyRuntimeIdentifier);Platform=$(LoginProxyPublishPlatform);PublishDir=$(LoginProxyPublishDir);PublishProtocol=$(LoginProxyPublishProtocol);TargetFramework=$(LoginProxyPublishTargetFramework);SelfContained=$(LoginProxyPublishSelfContained);" />
		<Delete Files="$(LoginProxyAssetsPath)" />
		<MakeDir Directories="$(MSBuildProjectDirectory)\Assets\Binary" />
		<ZipDirectory DestinationFile="$(LoginProxyAssetsPath)" SourceDirectory="$(LoginProxyPublishDir)" />
		<RemoveDir Directories="$(LoginProxyPublishDir)" />
	</Target>
