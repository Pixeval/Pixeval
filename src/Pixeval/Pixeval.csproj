<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net5.0-windows10.0.19041.0</TargetFramework>
		<UseWPF>true</UseWPF>
		<RuntimeIdentifier>win-x64</RuntimeIdentifier>
		<Description>A Strong, Fast and Flexible Pixiv Client</Description>
		<EnableDefaultApplicationDefinition>false</EnableDefaultApplicationDefinition>
		<ApplicationIcon>Resources\pxlogo.ico</ApplicationIcon>
		<Platforms>x64</Platforms>
		<NoWarn>$(NoWarn);NU1701</NoWarn>
		<StartupObject>Pixeval.Program</StartupObject>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Objects\I18n\Xml\resx_en-us.xml" />
		<None Remove="Objects\I18n\Xml\resx_zh-cn.xml" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="AdaptiveCards" Version="2.4.0" />
		<PackageReference Include="AdysTech.CredentialManager" Version="2.3.0" />
		<PackageReference Include="AngleSharp" Version="1.0.0-alpha-844" />
		<PackageReference Include="Dragablz" Version="0.0.3.223" />
		<PackageReference Include="EmbedIO" Version="3.4.3" />
		<PackageReference Include="Extended.Wpf.Toolkit" Version="4.0.1" />
		<PackageReference Include="Magick.NET-Q16-AnyCPU" Version="7.22.2.2" />
		<PackageReference Include="MahApps.Metro" Version="2.4.3" />
		<PackageReference Include="MaterialDesignColors" Version="2.0.0-ci2312" />
		<PackageReference Include="MaterialDesignThemes" Version="4.0.0-ci2312" />
		<PackageReference Include="Microsoft-WindowsAPICodePack-Shell" Version="1.1.4" />
		<PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="3.3.2">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.9.0-3.final" />
		<PackageReference Include="Microsoft.Web.WebView2" Version="1.0.721-prerelease" />
		<PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
		<PackageReference Include="PropertyChanged.Fody" Version="3.3.1" />
		<PackageReference Include="refit" Version="6.0.0-preview.37" />
		<PackageReference Include="SharpVectors" Version="1.7.1" />
		<PackageReference Include="sqlite-net-pcl" Version="1.8.0-beta" />
		<PackageReference Include="System.Interactive.Async" Version="5.0.0" />
		<PackageReference Include="VirtualizingWrapPanel" Version="1.5.3" />
	</ItemGroup>

	<ItemGroup>
		<Resource Include="Objects\I18n\Xml\resx_en-us.xml" />
		<Resource Include="Objects\I18n\Xml\resx_zh-cn.xml" />
		<Resource Include="Resources\dqueue.ttf" />
		<Resource Include="Resources\iconfont.ttf" />
		<Resource Include="Resources\iconfontex.ttf" />
		<Resource Include="Resources\interchange.zip" />
		<Resource Include="Resources\logo-only.svg" />
		<Resource Include="Resources\pixeval_ca.cer" />
		<Resource Include="Resources\pixeval_server_cert.pfx" />
		<Resource Include="Resources\pxlogo.ico" />
		<Resource Include="Resources\saucenao.png" />
	</ItemGroup>
</Project>
